<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>get crud</title>
    <style>
        table, th, td {
          border:1px solid black;
        }
        table {
            width: 100%;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>Email</th>
            <th>First name</th>
            <th>Last name</th>
            <th>Address</th>
            <th>Actions</th>
        </tr>
        <% for(var i=0; i < dataTable.length; i++) {%>
            <tr>
              <td><%= dataTable[i].email %></td>
              <td><%= dataTable[i].firstName %></td>
              <td><%= dataTable[i].lastName %></td>
              <td><%= dataTable[i].address %></td>
              <td>
                <a href="/edit-crud?id=<%= dataTable[i].id %>">Edit</a>
                <a href="/delete-crud?id=<%= dataTable[i].id %>">Delete</a>
              </td>
            </tr>
         <% } %>
    </table>
</body>
</html>